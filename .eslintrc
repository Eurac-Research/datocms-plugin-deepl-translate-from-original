{
  "parser": "babel-eslint",
  "extends": "prettier",
  "env": {
    "es6": true,
    "mocha": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "no-console": 0,
    "quotes": ["error", "double"]
  }
}
